{
  "name": "feathers-async-boot",
  "version": "0.1.1",
  "description": "booting feathersjs asynchron",
  "engines": {
    "node": ">= 10",
    "npm": ">= 5"
  },
  "main": "lib/",
  "types": "./index.d.ts",
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "scripts": {
    "compile": "rimraf lib/ && tsc",
    "dev": "ts-node-dev --no-notify src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "travis": "npm run test; npm run compile",
    "codecov": "jest && codecov",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idaho/feathers-async-boot.git"
  },
  "keywords": [
    "feathers",
    "async",
    "asynchron",
    "boot",
    "bootstrap",
    "running",
    "start"
  ],
  "author": "idaho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idaho/feathers-async-boot/issues"
  },
  "homepage": "https://github.com/idaho/feathers-async-boot#readme",
  "devDependencies": {
    "@feathersjs/express": "4.5.8",
    "@feathersjs/feathers": "4.5.8",
    "@types/jest": "26.0.13",
    "@types/node": "14.6.4",
    "jest": "26.4.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.3.0",
    "tslint": "6.1.3",
    "typescript": "4.0.2"
  },
  "dependencies": {}
}
